{
  "apiVersion": 2,
  "name": "ws/latest-upcoming",
  "title": "Latest & Upcoming",
  "description": "List latest posts of a chosen type or list upcoming post types with a start date.",
  "category": "ws-dynamic",
  "textdomain": "ws",
  "supports": {
    "anchor": true
  },
  "attributes": {
    "type": {
      "type": "string"
    },
    "postTypes": {
      "type": "array",
      "default": []
    },
    "taxTerms": {
      "type": "object",
      "default": {}
    },
    "allPosts": {
      "type": "boolean"
    },
    "numPosts": {
      "type": "number",
      "default": 3
    },
    "sideScroll": {
      "type": "boolean"
    }
  }
}
